{
    "jest.pathToJest": "node_modules/jest/bin/jest.js",
    "jest.pathToConfig": "jest.json",
    "cSpell.words": [
        "devtool",
        "lockfile",
        "parens",
        "shakirzianov",
        "template",
        "version",
        "webapp",
        "webpack",
        "webpack's"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules/**": true,
        "**/build/**": true,
        "package-lock.json": true
    },
    "tslint.autoFixOnSave": true
}